        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#a0a1a7"
            }
        },
        {
            "name": "Constants - Escape, Inheritance",
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#bd8100",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#e45649"
            }
